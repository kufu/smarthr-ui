    "@storybook/jest": "^0.1.0",
    "@storybook/react": "^7.2.3",
    "@storybook/react-webpack5": "^7.2.3",
    "@storybook/test-runner": "^0.13.0",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/theming": "^7.2.3",
    "@swc/core": "^1.3.78",
    "@swc/jest": "^0.2.29",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.3",
    "@types/lodash.merge": "^4.6.7",
    "@types/lodash.range": "^3.2.7",
    "@types/node": "^14.18.54",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/react-test-renderer": "^17.0.2",
    "@types/react-transition-group": "^4.4.6",
    "@types/styled-components": "^5.1.26",
    "axe-playwright": "^1.2.3",
    "babel-loader": "^9.1.3",
    "babel-plugin-polyfill-corejs2": "^0.4.5",
    "babel-plugin-polyfill-regenerator": "^0.5.2",
    "chromatic": "^6.22.0",
    "ecma-version-validator-webpack-plugin": "^1.2.1",
    "eslint": "^8.47.0",
    "eslint-config-smarthr": "^6.9.0",
    "eslint-plugin-storybook": "^0.6.13",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.3",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "jest-styled-components": "^7.1.1",
    "lint-staged": "^14.0.0",
    "memory-fs": "^0.5.0",
    "npm-run-all": "^4.1.5",
    "postcss-jsx": "^0.36.4",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.8.8",
    "prettier-config-smarthr": "^1.0.0",
    "puppeteer": "^21.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "smarthr-normalize-css": "^1.1.0",
    "standard-version": "^9.3.2",
    "storybook": "^7.2.3",
    "styled-components": "^5.3.11",
    "styled-reset": "^4.5.1",
    "stylelint": "^15.10.2",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-smarthr": "^1.1.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "testcafe": "3.2.0",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "ttypescript": "^1.5.15",
    "typescript": "^5.1.6",
    "typescript-plugin-styled-components": "^3.0.0",
    "wait-on": "^7.0.1",
    "webpack": "^5.88.2"
  },
  "peerDependencies": {
    "react": "16.13.0 || ^17.0.1 || ^18.0.0",
    "react-dom": "16.13.0 || ^17.0.1 || ^18.0.0",
    "styled-components": "^5.0.1"
  },
  "bugs": {
    "url": "https://github.com/kufu/smarthr-ui/issues"
  },
  "files": [
    "esm",
    "lib"
  ],
  "homepage": "https://github.com/kufu/smarthr-ui#readme",
  "keywords": [
    "react",
    "react-component",
    "smarthr",
    "typescript"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "main": "lib/index.js",
  "module": "esm/index.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kufu/smarthr-ui.git"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:lib": "tsc -p tsconfig.build.json",
    "build:esm": "tsc -p tsconfig.esm.build.json",
    "build-storybook": "storybook build --quiet -s ./public",
    "build-stylesheet": "ttsc -p tsconfig.stylesheet.json; node scripts/build-stylesheet.ts",
    "clean": "rimraf ./lib",
    "format": "eslint --fix './**/*.ts{,x}'",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint './**/*.ts{,x}'",
    "lint:style": "stylelint './**/*.ts{,x}'",
    "lint:tsc": "tsc --noEmit",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "prepublishOnly": "run-s clean lint build",
    "release": "standard-version -a",
    "release:dryrun": "standard-version --dry-run",
    "storybook": "storybook dev -p 6006",
    "dev": "run-s storybook",
    "test": "jest",
    "test:update-snapshot": "jest --updateSnapshot",
    "test:build-assets": "node scripts/build-test.ts",
    "test-storybook:ci": "wait-on tcp:6006 && yarn test-storybook --maxWorkers=2 --junit",
    "testcafe": "testcafe",
    "e2e": "ts-node scripts/e2e.ts",
    "e2e:dev": "testcafe chrome --host localhost --skip-js-errors",
    "prepare": "husky install",
    "chromatic": "chromatic",
    "write:ui-props": "ts-node scripts/exportUIProps.ts",
    "export:ui-props": "run-s build:lib write:ui-props"
  },
  "sideEffects": false,
  "typings": "lib/index.d.ts",
  "resolutions": {
    "@babel/helper-compilation-targets": "^7.22.10",
    "@types/react": "^18.2.20",
    "minimist": "1.2.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "standard-version": {
    "scripts": {
      "prerelease": "run-s build:lib write:ui-props && git add public/exports/smarthr-ui-props.json"
    }
  }
}
