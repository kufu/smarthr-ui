{
  "name": "smarthr-ui",
  "description": "SmartHR ui components built with React.",
  "version": "14.2.0",
  "author": "SmartHR-UI Team",
  "dependencies": {
    "dayjs": "^1.10.6",
    "lodash.merge": "^4.6.2",
    "lodash.range": "^3.2.0",
    "polished": "^4.1.3",
    "react-draggable": "^4.4.3",
    "react-icons": "^4.2.0",
    "react-transition-group": "^4.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@storybook/addon-a11y": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addons": "^6.3.8",
    "@storybook/cli": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "@storybook/theming": "^6.3.8",
    "@types/jest": "^27.0.1",
    "@types/lodash.merge": "^4.6.6",
    "@types/lodash.range": "^3.2.6",
    "@types/node": "^14.17.14",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/react-test-renderer": "^17.0.1",
    "@types/react-transition-group": "^4.4.2",
    "@types/styled-components": "^5.1.14",
    "babel-loader": "^8.2.2",
    "ecma-version-validator-webpack-plugin": "^1.1.0",
    "eslint": "^7.32.0",
    "eslint-config-smarthr": "^5.0.3",
    "http-server": "^13.0.1",
    "husky": "^7.0.2",
    "jest": "^27.1.0",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^11.1.2",
    "memory-fs": "^0.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "prettier-config-smarthr": "^1.0.0",
    "puppeteer": "^10.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "reg-keygen-git-hash-plugin": "^0.10.16",
    "reg-notify-github-plugin": "^0.10.16",
    "reg-publish-s3-plugin": "^0.10.16",
    "reg-suit": "^0.10.16",
    "standard-version": "^9.3.1",
    "storybook-readme": "^5.0.9",
    "storycap": "^3.1.0",
    "styled-components": "^5.3.1",
    "styled-reset": "^4.3.4",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-smarthr": "^1.0.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "testcafe": "^1.15.3",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2",
    "wait-for-localhost": "^3.3.0",
    "webpack": "^5.52.0"
  },
  "peerDependencies": {
    "react": "16.13.0 || ^17.0.1",
    "react-dom": "16.13.0 || ^17.0.1",
    "styled-components": "^5.0.1"
  },
  "bugs": {
    "url": "https://github.com/kufu/smarthr-ui/issues"
  },
  "engines": {
    "node": ">=v12"
  },
  "files": [
    "esm",
    "lib"
  ],
  "homepage": "https://github.com/kufu/smarthr-ui#readme",
  "keywords": [
    "react",
    "react-component",
    "smarthr",
    "typescript"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "main": "lib/index.js",
  "module": "esm/index.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kufu/smarthr-ui.git"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:lib": "tsc -p tsconfig.build.json",
    "build:esm": "tsc -p tsconfig.esm.build.json",
    "build-storybook": "build-storybook --quiet -s ./public",
    "clean": "rimraf ./lib",
    "format": "eslint --fix './**/*.ts{,x}'",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint './**/*.ts{,x}'",
    "lint:style": "stylelint './**/*.ts{,x}'",
    "lint:tsc": "tsc --noEmit",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "prepublishOnly": "run-s clean lint build",
    "pretest-visual": "build-storybook -o reg_storybook --quiet -s ./public; storycap --serverCmd \"npx http-server reg_storybook -p 6006\" http://localhost:6006 -o actual_images  --serverTimeout 3600000",
    "release": "standard-version",
    "release:dryrun": "standard-version --dry-run",
    "storybook": "start-storybook -p 6006 -s ./public",
    "test": "jest",
    "test:update-snapshot": "jest --updateSnapshot",
    "test:build-assets": "node scripts/build-test.ts",
    "test-visual": "reg-suit run",
    "testcafe": "testcafe",
    "e2e": "ts-node scripts/e2e.ts",
    "e2e:dev": "testcafe chrome --host localhost --skip-js-errors",
    "prepare": "husky install"
  },
  "sideEffects": false,
  "typings": "lib/index.d.ts",
  "resolutions": {
    "reg-suit/**/braces": "3.0.2",
    "minimist": "1.2.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "@types/react": "^17.0.20",
    "@babel/helper-compilation-targets": "^7.15.4"
  }
}
